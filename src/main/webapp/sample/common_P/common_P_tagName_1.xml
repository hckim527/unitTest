		<script lazy="false" type="text/javascript"><![CDATA[let objParent = $p.parent();

let cTagName = '';



// 테스트 설명, 유효성 값 설정
scwin.tData = {
    "description" : "[common > P > tagName]<br/>화면 렌더링 시 컴포넌트에 적용할 Tag 를 설정합니다.",
    "validation" : [
        "적용할 Tag 이름을 열거형으로 설정할 수 있어야 합니다.",
        "값을 설정하지 않으면 div Tag 가 적용되어야 합니다.",
    ]
}



scwin.onpageload = function () {
    $c.gcm.createCommonDatalist();

    // 테스트 설명, 유효성 생성
    wf_body_left.getObj("scwin").createValidation(scwin.tData);
};



// comp 재생성
scwin.comp_init = function () {
    // 조건 설정
    cTagName = con_tagname.getText();    

    // 테스트 샘플에 재생성 조건 값 전달
    objParent.scwin.gVar["cTagName"] = cTagName;
    
    // comp 생성 호출
    objParent.scwin.comp_init();
};



scwin.btn_getTagName_onclick = function (e) {
    let rtn = '';
    
    rtn = objParent.target1.render.tagName
    wf_body_bottom.getObj("scwin").setReturnValue("tagName : " + rtn);    
};
]]></script>
