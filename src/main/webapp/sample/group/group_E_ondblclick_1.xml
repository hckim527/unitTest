		<script lazy="false" type="text/javascript"><![CDATA[// 테스트 설명, 유효성 값 설정
scwin.tData = {
    "description" : "[group > E > onclick]<br/>group 컴포넌트를 마우스 왼쪽 버튼으로 더블 클릭할 때 발생합니다.",
    "validation" : [
        "onclick 이벤트가 두 번 발생한 후 ondblclick 이벤트가 발생합니다.",
        "group 의 자식 컴포넌트에서 ondblclick 이벤트가 발생하면 이벤트 버블링을 통해 group 컴포넌트도 이벤트가 발생하여야 합니다."
    ]
} 



scwin.onpageload = function () {
    $c.gcm.createCommonDatalist();

    // 테스트 설명, 유효성 생성
    wf_body_left.getObj("scwin").createValidation(scwin.tData);

    // comp 초기 생성
    scwin.comp_init();
};



// comp 생성
scwin.comp_init = async function () {
    // comp 렌더링 할 객체 가져오기    
    if (typeof wf_sample !== 'undefined') {
        let wf_body_sample = wf_sample.getObj("wf_body_sample");
        grp_comp = wf_body_sample.getObj("grp_comp");
    }
    else {        
        grp_comp = wf_body_sample.getObj("grp_comp"); 
    }

    // 기존 target 있으면 삭제
    if (typeof target1 !== 'undefined') {
        target1.remove();
    }

    // comp 생성 함수 호출    
    $c.gcm.createBasicGroup(grp_comp, '', 'ev:onclick="scwin.target1_onclick" ev:ondblclick="scwin.target1_ondblclick"');    
}



scwin.target1_onclick = function (e) {
    wf_body_bottom.getObj("scwin").setReturnValue("=====target1_onclick 이벤트 발생=====");
    wf_body_bottom.getObj("scwin").setReturnValue("e:" + e);   
}


scwin.target1_ondblclick = function (e) {
    wf_body_bottom.getObj("scwin").setReturnValue("=====target1_ondblclick 이벤트 발생=====");
    wf_body_bottom.getObj("scwin").setReturnValue("e:" + e);   
}]]></script>
